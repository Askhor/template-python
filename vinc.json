{
	"template": "<YEAR>.<MONTH>.<COUNTER>",
	"version": "0.0.0",
	"targets": {
		"pyproject.toml": {
			"find": "\"<VERSION>\"\\s*#\\s*version",
			"replace": "\"<VERSION>\" # version"
		},
		"placeholder/__init__.py": {
			"find": "__version__\\s*=\\s*\"<VERSION>\"\\s*#\\s*version",
			"replace": "__version__ = \"<VERSION>\"  # version"
		}
	}
}